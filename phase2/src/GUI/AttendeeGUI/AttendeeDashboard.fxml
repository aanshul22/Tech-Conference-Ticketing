<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="gridPane" alignment="BASELINE_LEFT" prefHeight="650.0" prefWidth="900.0" snapToPixel="false" styleClass="root" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.AttendeeGUI.AttendeeDashboardController">
    <VBox fillWidth="false" prefHeight="575.0" prefWidth="346.0" snapToPixel="false" spacing="50" styleClass="VBox" stylesheets="@AttendeeDashboard.css">
        <Label fx:id="profile" contentDisplay="CENTER" textAlignment="CENTER" />
        <VBox prefHeight="535.0" prefWidth="223.0" snapToPixel="false" spacing="400">
            <VBox spacing="30.0">
                <Button fx:id="availEventButton" onAction="#handleAvailEventButtonAction" prefHeight="33.0" prefWidth="134.0" text="Available Events">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Images/Icons/event_w.png" />
                     </image></ImageView>
               </graphic></Button>
                <Button fx:id="myEventButton" graphicTextGap="0.0" onAction="#handleMyEventButtonAction" prefHeight="33.0" prefWidth="150" text="My Events">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Images/Icons/view_w.png" />
                     </image></ImageView>
               </graphic></Button>
                <Button fx:id="messageButton" onAction="#handleMessageButtonAction" prefHeight="33.0" prefWidth="150" text="Message">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Images/Icons/conversation_w.png" />
                     </image></ImageView>
               </graphic></Button>
            </VBox>
        </VBox>
    </VBox>
   <Pane prefHeight="120.0" prefWidth="130.0" snapToPixel="false" styleClass="VBox" stylesheets="@AttendeeDashboard.css" GridPane.rowIndex="1">
      <children>

            <Button fx:id="logOutButton" alignment="BOTTOM_CENTER" layoutY="2.0" mnemonicParsing="false" onAction="#handleLogOutButtonAction" prefHeight="2.0" prefWidth="150.0" stylesheets="@AttendeeDashboard.css" text="Log Out">
            <graphic>
               <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@../Images/Icons/log-out_w.png" />
                  </image>
               </ImageView>
            </graphic></Button>
         <Pane prefHeight="200.0" prefWidth="200.0" styleClass="VBox" stylesheets="@AttendeeDashboard.css" />
      </children>
      <GridPane.margin>
         <Insets />
      </GridPane.margin>
   </Pane>
    <Pane prefHeight="93.0" prefWidth="587.0" snapToPixel="false" GridPane.columnIndex="1" GridPane.rowIndex="1">
      <children>
          <Button fx:id="cancelButton" layoutX="543.0" layoutY="2.0" onAction="#handleCancelButtonAction" prefHeight="25.0" prefWidth="176.0" snapToPixel="false" stylesheets="@AttendeeDashboard.css" text="Cancel Registration" />
      </children></Pane>
    <stylesheets>
        <URL value="@AttendeeDashboard.css" />
    </stylesheets>
   <columnConstraints>
      <ColumnConstraints maxWidth="320" minWidth="150" prefWidth="150" />
      <ColumnConstraints maxWidth="740" minWidth="570" prefWidth="740" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
